@model List<Projeto.NET_MVC.Models.Aluguel>
@{
    ViewBag.Title = "Rents";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Rents</h2>
@if (Model.Count == 0)
{
    <text>We don't have any rents yet.</text>
}
else
{
    <ul>
        @foreach (var rent in Model)
        {
        <li>@rent.Id_customer - @rent.Id_movie</li>
            @*
            <li>@Html.ActionLink(rent.Id_customer + " " + rent.Id_movie, "Details/" + rent.Id, "Movies")</li>
            *@
        }
    </ul>
}